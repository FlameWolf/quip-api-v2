		"Post": {
			"content": {
				"multipart/form-data": {
					"schema": {
						"type": "object",
						"properties": {
							"content": {
								"type": "string",
								"format": "textarea"
							},
							"poll": {
								"type": "object",
								"properties": {
									"first": {
										"required": true,
										"type": "string"
									},
									"second": {
										"required": true,
										"type": "string"
									},
									"third": {
										"type": "string"
									},
									"fourth": {
										"type": "string"
									},
									"duration": {
										"type": "integer",
										"format": "int64",
										"minimum": 1800000,
										"maximum": 604800000,
										"default": 86400000
									}
								}
							},
							"media": {
								"type": "file"
							},
							"media-description": {
								"type": "string"
							},
							"location": {
								"type": "object",
								"properties": {
									"type": {
										"required": true,
										"type": "string",
										"enum": ["Point"]
									},
									"coordinates": {
										"required": true,
										"type": "array",
										"items": {
											"required": true,
											"type": "integer",
											"minimum": -180,
											"maximum": 180
										}
									}
								}
							}
						}
					}
				}
			}
		},
		"WordMute": {
			"content": {
				"application/json": {
					"schema": {
						"type": "object",
						"properties": {
							"word": {
								"type": "string"
							},
							"match": {
								"type": "string",
								"enum": ["exact", "contains", "startsWith", "endsWith"]
							}
						}
					}
				}
			}
		},
		"FollowRequests": {
